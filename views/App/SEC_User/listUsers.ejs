<div class="col-md-12">
	<div class="row">
		<div class="col-md-6">
			<h2>List Users</h2>
		</div>
	</div>
	<table class="table table-striped table-bordered" id="table">
		<thead>
			<tr>
				<th>#</th>
				<th>UserName</th>
				<th>Name</th>
				<th>Last Name</th>
				<th>Phone</th>
				<th>Email</th>
				<th>Roles</th>
				<th>Actions</th>
			</tr>
		</thead>
		<tbody>
			<% for(let i=0, len=data.length; i < len; i++) { %>
				<% let item = data[i] %>
				<tr>
					<td><%= i+1 %></td>
					<td><%= item.userName %></td>
					<td><%= item.name %></td>
					<td><%= item.lastName %></td>
					<td><%= item.phone %></td>
					<td><%= item.email %></td>
					<td><ul class="list-inline">
					<% for(let j=0, len2=item.roles.length; j < len2; j++) { %>
						<li><%= item.roles[j].name %></li>
						<% if(j+1 < item.roles.length){ %>
							&bull;
						<% }; %>
					<% }; %>
					</ul></td>
					<td>
						<% let editURL= '/editUser/' + item.id %>
						<% if(SEC_SecurityService.verifyPermissionsToPath(res, editURL)){  %>
							<a style="color:green" href="<%= editURL %>">Edit</a>
						<% } %>
					</td>
			<% }; %>
		</tbody>
	</table>
</div>